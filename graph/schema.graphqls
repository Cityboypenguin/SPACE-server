# GraphQL schema example
#
# https://gqlgen.com/getting-started/

scalar DateTime

type User {
  id: ID!
  name: String!
  email: String!
  createdAt: DateTime!
  updatedAt: DateTime!
}

input signUpInput {
  name: String!
}

type Query {
  hello: String!
  users: [User!]!
}

type Mutation {
  signUp(in: signUpInput!): User!
}
